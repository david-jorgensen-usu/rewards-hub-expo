{
  "expo": {
    "name": "RewardsHub",
    "slug": "rewardshub",
    "version": "1.0.0",
    "scheme": "rewardshub",
    "icon": "./assets/icons/rewardshub-icon-rescaled.png",

    "splash": {
      "image": "./assets/icons/rewardshub-splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "bundleIdentifier": "com.rewardshubonline.rewardshub",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "VIBRATE",
        "POST_NOTIFICATIONS",
        "INTERNET",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/rewardshub-icon-square.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rewardshubonline.rewardshub"
    },

    "extra": {
      "eas": {
        "projectId": "ee9e03d6-b71f-44b6-abfc-f9c7f5e7d664"
      }
    },

    "plugins": [
      "expo-secure-store"
    ]
  }
}
